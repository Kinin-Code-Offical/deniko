#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/build/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/build/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules:/mnt/c/BASE/DENIKO-PROJECT/deniko/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/build/index.js" "$@"
else
  exec node  "$basedir/../../../../../../prisma@7.1.0_@types+react@1_0c5fec34ff24eec95f93a5f07aa60ed1/node_modules/prisma/build/index.js" "$@"
fi
